{
  "version": 2,
  "buildCommand": "next build",
  "devCommand": "next dev",
  "framework": "nextjs",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://searchit-backend.onrender.com/api"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://searchit-backend.onrender.com/api/:path*"
    }
  ]
} 